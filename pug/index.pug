doctype html
html(lang="ja")
  head
    meta(charset="utf-8")
      
    link(href="../css/reset.css" rel="stylesheet")
    link(href="../css/jquery.fullPage.css" rel="stylesheet")
    link(href="../css/style.css" rel="stylesheet")
      
    script(src="../js/vendors/jquery-1.11.2.min.js")
    script(src="../js/vendors/jquery.easings.min.js")
    script(src="../js/vendors/scrolloverflow.min.js")
    script(src="../js/vendors/jquery.fullPage.min.js")
    script(src="../js/main.js")
      
    title 産学連携サンプル
  body
    include include/_header.pug
    #fullpage.fullpage(style="display: none;")
      section.section.top_menu#section1
        .logo
          p.logo__item
            a.logo__item__link(href="#section1")
              img.menu_logo_image(src="../img/menu_logo.png" alt="メニューロゴ")
        ul.list.clearfix
          li.list__item
            a.list__item__link(href="#page_1")
              img.list__item__link__image(src="../img/huzitaya.png" alt="藤田屋1")
          li.list__item
            a.list__item__link(href="#page_2")
              img.list__item__link__image(src="../img/huzitaya.png" alt="藤田屋2")
          li.list__item
            a.list__item__link(href="#page_3")
              img.list__item__link__image(src="../img/huzitaya.png" alt="藤田屋3")
          li.list__item
            a.list__item__link(href="#page_4")
              img.list__item__link__image(src="../img/huzitaya.png" alt="藤田屋4")
      section.section#section2
        .slide#slide2_1 1店舗目スライド1
        .slide#slide2_2 1店舗目スライド2
        .slide#slide2_3 1店舗目スライド3
        .slide#slide2_4 1店舗目スライド4
      section.section#section3
        .slide.slide3_1#slide3_1
          .back_layer
            .quiz
              .quiz__word
                .question
                  p.question__word
                    span 名古屋市中央卸売市場では
                    span 一般の人がセリに参加することができる！
                  .question__circle
                    p.question__circle__number Q1
              .quiz__choice
        .slide#slide3_2
          //  ←ここに.fp-tableCellが入る
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
        .slide#slide3_3 スライド3
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
        .slide#slide3_4 スライド4
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
        .slide#slide3_5 スライド5
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
          img(src="../img/jk01.png" alt="おこ")
      section.section#section4
        .slide#slide4_1 3店舗目スライド1
        .slide#slide4_2 3店舗目スライド2
        .slide#slide4_3 3店舗目スライド3
        .slide#slide4_4 3店舗目スライド4
        .slide#slide4_5 3店舗目スライド5
      section.section#section5
        .slide#slide5_1 4店舗目スライド1
        .slide#slide5_2 4店舗目スライド2
        .slide#slide5_3 4店舗目スライド3
        .slide#slide5_4 4店舗目スライド4
        .slide#slide5_5 4店舗目スライド5
